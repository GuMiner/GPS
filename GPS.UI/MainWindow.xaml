<Window x:Class="GPS.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GPS.UI"
        mc:Ignorable="d"
        Title="Word Search" Height="350" Width="525">
    <Grid Background="LightBlue">
        <TextBox x:Name="searchBox" Height="23" Margin="10,10,10,0" TextWrapping="Wrap" Text="Demo*" VerticalAlignment="Top" TextChanged="searchBox_TextChanged"/>
        <Label x:Name="statusLabel" Content="Loeading..." Background="LightYellow" Margin="10,0,10,4" Height="26" VerticalAlignment="Bottom"/>
        <ListBox x:Name="resultsBox" Margin="10,38,10,35"/>
    </Grid>
</Window>
